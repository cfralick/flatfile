{
    "name": "cfralick/csv",
    "description": "readable & writeable csv",
    "license": "MIT",
    "authors": [
        {
            "name": "Clint Fralick",
            "role": "Contributor"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/filesystem": ">=2.4.0",
        "keboola/csv": ">=1.1.3"
    },
    "autoload": {
        "psr-4": {
            "Csv\\": "src/Csv/",
            "Csv\\Tests\\": "tests/Csv/Tests/"
        },
        "psr-0": {
            "Csv\\": "src/",
            "Csv\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": ">=3.7.0",
        "phpunit/php-invoker": ">=1.1.3"
    }
}
