{
    "name": "cfralick/flatfile",
    "description": "readable & writeable flatfiles",
    "license": "BSD",
    "version": "0.0.0",
    "authors": [
        {
            "name": "Clint Fralick",
            "role": "Contributor"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "keboola/csv": ">=1.1.3"
    },
    "autoload": {
        "psr-4": {
            "FlatFile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {"FlatFile\\Tests\\": "tests/"}
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0",
        "phpunit/php-invoker": ">=1.1.3"
    }
}
